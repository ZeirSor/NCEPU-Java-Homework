/*
 * 1．设计Point类用来定义平面上的一个点坐标，包含向右平移和向下平移的方法。
 * 	  编写测试类Test，在该类中定义对象，并调用相应的函数。
 */

package Point;

public class Point {
	private double x;
	private double y;

	public void moveRight(double dis) {
		this.x += dis;
		
	}

	public void moveDown(double dis) {
		this.y -= dis;
	}

	public Point(double x, double y) {
		super();
		this.x = x;
		this.y = y;
	}

	public Point() {
		super();
		// TODO Auto-generated constructor stub
	}

	public double getX() {
		return x;
	}

	public void setX(double x) {
		this.x = x;
	}

	public double getY() {
		return y;
	}

	public void setY(double y) {
		this.y = y;
	}

	@Override
	public String toString() {
		return "Point (x, y) = " + "(" + x + ", " + y + ")";
	}

}
